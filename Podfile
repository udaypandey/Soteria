# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'Soteria' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Soteria

  # Pods for RX
  pod 'RxSwift',    '~> 4.4.1'
  pod 'RxCocoa',    '~> 4.4.1'

  # Pods for Networking
  pod 'Moya', '~> 12.0'
  pod 'Alamofire', '~> 4.8.1'

  target 'SoteriaTests' do
    inherit! :search_paths
    # Pods for testing

    # RX Testing
    pod 'RxTest',    '~> 4.4.1'
    pod 'RxBlocking',    '~> 4.4.1'
  end

  target 'SoteriaUITests' do
    inherit! :search_paths
    # Pods for testing
  end
end


